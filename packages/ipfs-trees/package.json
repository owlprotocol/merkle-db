{
  "name": "@owlprotocol/ipfs-trees",
  "version": "0.1.6",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/owlprotocol/ipfs-trees.git"
  },
  "homepage": "https://leovigna.github.io/ipfs-trees",
  "copyright": "Copyright 2021 Leo Vigna",
  "license": "MIT",
  "files": [
    "*",
    "!test",
    "!./**/*.test.*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "publishConfig": {
    "directory": "lib"
  },
  "scripts": {
    "clean": "rimraf lib",
    "test": "nyc mocha 'lib/**/*.test.js'",
    "start": "node lib/index.js",
    "build": "npm-run-all tsc copy",
    "tsc": "tsc",
    "tsc:watch": "tsc -w",
    "copy": "npm-run-all copy:prepublish",
    "copy:prepublish": "mkdir -p lib && cp package.json lib/ && cp README.md lib/ && cp LICENSE lib/",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "lint:staged": "lint-staged",
    "postinstall": "npm run build",
    "prepublishOnly": "npm run test"
  },
  "devDependencies": {
    "@multiformats/sha3": "^2.0.8",
    "@types/chai": "^4.3.0",
    "@types/lodash": "^4.14.180",
    "@types/mocha": "^9.1.0",
    "@types/node": "^16.11.26",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "chai": "^4.3.6",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^7.0.4",
    "ipfs": "^0.62.1",
    "ipfs-core": "^0.14.1",
    "keccak256": "^1.0.6",
    "lint-staged": "^11.2.6",
    "lodash": "^4.17.21",
    "merkletreejs": "^0.2.31",
    "mocha": "^9.2.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.6.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@ipld/dag-cbor": "^7.0.1",
    "@ipld/dag-json": "^8.0.8",
    "multiformats": "^9.6.4",
    "tiny-invariant": "^1.2.0"
  }
}
